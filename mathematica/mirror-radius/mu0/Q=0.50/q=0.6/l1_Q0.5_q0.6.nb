Cell[BoxData[
 RowBox[{
  RowBox[{"ie", "=", "0"}], ";", 
  RowBox[{"While", "[", 
   RowBox[{
    RowBox[{"ie", "<", "101"}], ",", 
    RowBox[{
     RowBox[{"NITMAX", "=", "250"}], ";", "\[IndentingNewLine]", 
