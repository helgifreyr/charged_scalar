         RowBox[{"wc", "=", 
          RowBox[{"qscal", "*", 
           RowBox[{"Q", "/", "rp"}]}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"qsol", "=", 
          RowBox[{"-", 
