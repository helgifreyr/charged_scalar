         RowBox[{"r1", "=", 
          RowBox[{"30", "+", 
           RowBox[{"ie", "/", "5"}]}]}], ";", " ", 
         RowBox[{"rm", "=", 
          RowBox[{"M", "-", 
           RowBox[{"Sqrt", "[", 
            RowBox[{
             RowBox[{"M", "^", "2"}], "-", 
