                  RowBox[{
                   RowBox[{"Rs", "[", "r", "]"}], ",", 
                   RowBox[{"{", 
                    RowBox[{"r", ",", "2"}], "}"}]}], "]"}]}], "+", 
                RowBox[{
                 RowBox[{"(", "   ", 
                  RowBox[{
                   RowBox[{
                    RowBox[{"2", "/", "r"}], "*", 
                    RowBox[{"(", " ", 
                    RowBox[{"1", "-", 
                    RowBox[{"2", "*", 
                    RowBox[{"M", "/", "r"}]}], "+", 
                    RowBox[{
                    RowBox[{"Q", "^", "2"}], "/", 
                    RowBox[{"r", "^", "2"}]}]}], ")"}]}], " ", "+", " ", 
                   RowBox[{"(", "  ", 
                    RowBox[{
                    RowBox[{"2", "*", 
                    RowBox[{"M", "/", 
                    RowBox[{"r", "^", "2"}]}]}], "-", 
                    RowBox[{"2", "*", 
                    RowBox[{
                    RowBox[{"Q", "^", "2"}], "/", 
                    RowBox[{"r", "^", "3"}]}]}]}], "  ", ")"}]}], " ", ")"}], 
                 "*", 
                 RowBox[{"D", "[", 
                  RowBox[{
                   RowBox[{"Rs", "[", "r", "]"}], ",", 
                   RowBox[{"{", 
                    RowBox[{"r", ",", "1"}], "}"}]}], "]"}]}], "+", 
                RowBox[{
                 RowBox[{"(", "     ", 
                  RowBox[{
                   RowBox[{
                    RowBox[{"1", "/", 
                    RowBox[{"(", 
                    RowBox[{"1", "-", 
                    RowBox[{"2", "*", 
                    RowBox[{"M", "/", "r"}]}], "+", 
                    RowBox[{
                    RowBox[{"Q", "^", "2"}], "/", 
                    RowBox[{"r", "^", "2"}]}]}], ")"}]}], "*", 
                    RowBox[{
                    RowBox[{"(", "  ", 
                    RowBox[{"w", "-", 
                    RowBox[{"qscal", "*", 
                    RowBox[{"Q", "/", "r"}]}]}], " ", ")"}], "^", "2"}]}], 
                   "-", 
                   RowBox[{"l", "*", 
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"l", "+", "1"}], ")"}], "/", 
                    RowBox[{"r", "^", "2"}]}]}], "-", 
                   RowBox[{"mu", "^", "2"}]}], "        ", ")"}], "*", 
                 RowBox[{"Rs", "[", "r", "]"}]}]}], "==", "0"}], ",", 
              RowBox[{
               RowBox[{"Rs", "[", "r0", "]"}], "\[Equal]", 
               RowBox[{
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"rp", "-", "rm"}], ")"}], "^", 
                 RowBox[{"(", 
                  RowBox[{"sigmasol", "-", "rhosol", "-", "1"}], ")"}]}], "*", 
                RowBox[{"Exp", "[", 
                 RowBox[{"qsol", "*", "r0"}], "]"}], "*", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"r0", "-", "rp"}], ")"}], "^", 
                 RowBox[{"(", "rhosol", ")"}]}]}]}], ",", 
              RowBox[{
               RowBox[{
                SuperscriptBox["Rs", "\[Prime]",
                 MultilineFunction->None], "[", "r0", "]"}], "\[Equal]", 
               RowBox[{
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"rp", "-", "rm"}], ")"}], "^", 
                 RowBox[{"(", 
                  RowBox[{"sigmasol", "-", "rhosol", "-", "1"}], ")"}]}], "*", 
                RowBox[{"Exp", "[", 
                 RowBox[{"qsol", "*", "r0"}], "]"}], "*", 
                RowBox[{"(", "rhosol", ")"}], "*", 
