Cell[BoxData[{
 RowBox[{
  RowBox[{"winit", "=", 
   RowBox[{"(", 
    RowBox[{"0.3", "-", 
     RowBox[{"0.0001", "*", "I"}]}], ")"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ie", "=", "0"}], ";", 
  RowBox[{"While", "[", 
   RowBox[{
    RowBox[{"ie", "<", "101"}], ",", 
    RowBox[{
     RowBox[{"NITMAX", "=", "250"}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"LeavP1", "[", 
       RowBox[{"w_", "?", "NumberQ"}], "]"}], ":=", "\[IndentingNewLine]", 
      RowBox[{"Module", "[", 
       RowBox[{
        RowBox[{"{", "Res", "}"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"r0", "=", 
          RowBox[{"rp", "+", 
           RowBox[{"10", "^", 
            RowBox[{"(", 
             RowBox[{"-", "8"}], ")"}]}]}]}], ";", 
         RowBox[{"r1", "=", 
          RowBox[{"30", "+", 
           RowBox[{"ie", "/", "5"}]}]}], ";", " ", 
         RowBox[{"rm", "=", 
          RowBox[{"M", "-", 
           RowBox[{"Sqrt", "[", 
            RowBox[{
             RowBox[{"M", "^", "2"}], "-", 
             RowBox[{"Q", "^", "2"}]}], "]"}]}]}], ";", " ", 
         RowBox[{"rp", "=", 
          RowBox[{"M", "+", 
           RowBox[{"Sqrt", "[", 
            RowBox[{
             RowBox[{"M", "^", "2"}], "-", 
             RowBox[{"Q", "^", "2"}]}], "]"}]}]}], " ", ";", 
         RowBox[{"wc", "=", 
          RowBox[{"qscal", "*", 
           RowBox[{"Q", "/", "rp"}]}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"qsol", "=", 
          RowBox[{"-", 
           RowBox[{"Sqrt", "[", 
            RowBox[{
             RowBox[{"mu", "*", "mu"}], "-", 
             RowBox[{"w", "*", "w"}]}], "]"}]}]}], " ", ";", 
         "\[IndentingNewLine]", 
         RowBox[{"rhosol", "=", 
          RowBox[{
           RowBox[{"-", "I"}], "*", 
           RowBox[{"rp", "^", "2"}], "*", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"w", "-", "wc"}], ")"}], "/", 
            RowBox[{"(", 
             RowBox[{"rp", "-", "rm"}], ")"}]}]}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"sigmasol", "=", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"qscal", "*", "Q", "*", "w"}], "+", 
            RowBox[{"M", "*", 
