Cell[BoxData[{
 RowBox[{
  RowBox[{"winit", "=", 
   RowBox[{"(", 
    RowBox[{"0.3", "-", 
     RowBox[{"0.0001", "*", "I"}]}], ")"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ie", "=", "0"}], ";", 
  RowBox[{"While", "[", 
   RowBox[{
    RowBox[{"ie", "<", "101"}], ",", 
    RowBox[{
     RowBox[{"NITMAX", "=", "250"}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"LeavP1", "[", 
       RowBox[{"w_", "?", "NumberQ"}], "]"}], ":=", "\[IndentingNewLine]", 
      RowBox[{"Module", "[", 
       RowBox[{
        RowBox[{"{", "Res", "}"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"r0", "=", 
          RowBox[{"rp", "+", 
           RowBox[{"10", "^", 
            RowBox[{"(", 
             RowBox[{"-", "8"}], ")"}]}]}]}], ";", 
         RowBox[{"r1", "=", 
          RowBox[{"30", "+", 
           RowBox[{"ie", "/", "5"}]}]}], ";", " ", 
         RowBox[{"rm", "=", 
          RowBox[{"M", "-", 
           RowBox[{"Sqrt", "[", 
            RowBox[{
             RowBox[{"M", "^", "2"}], "-", 
             RowBox[{"Q", "^", "2"}]}], "]"}]}]}], ";", " ", 
         RowBox[{"rp", "=", 
          RowBox[{"M", "+", 
           RowBox[{"Sqrt", "[", 
            RowBox[{
             RowBox[{"M", "^", "2"}], "-", 
             RowBox[{"Q", "^", "2"}]}], "]"}]}]}], " ", ";", 
         RowBox[{"wc", "=", 
          RowBox[{"qscal", "*", 
           RowBox[{"Q", "/", "rp"}]}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"qsol", "=", 
          RowBox[{"-", 
           RowBox[{"Sqrt", "[", 
            RowBox[{
             RowBox[{"mu", "*", "mu"}], "-", 
             RowBox[{"w", "*", "w"}]}], "]"}]}]}], " ", ";", 
         "\[IndentingNewLine]", 
         RowBox[{"rhosol", "=", 
          RowBox[{
           RowBox[{"-", "I"}], "*", 
           RowBox[{"rp", "^", "2"}], "*", 
           RowBox[{
            RowBox[{"(", 
             RowBox[{"w", "-", "wc"}], ")"}], "/", 
            RowBox[{"(", 
             RowBox[{"rp", "-", "rm"}], ")"}]}]}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"sigmasol", "=", 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"qscal", "*", "Q", "*", "w"}], "+", 
            RowBox[{"M", "*", 
             RowBox[{"mu", "^", "2"}]}], "-", 
            RowBox[{"2", "*", "M", "*", 
             RowBox[{"w", "^", "2"}]}]}], ")"}]}], ";", "\[IndentingNewLine]",
          "\[IndentingNewLine]", 
         RowBox[{"sol", "=", 
          RowBox[{"NDSolve", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"1", "-", 
                   RowBox[{"2", "*", 
                    RowBox[{"M", "/", "r"}]}], "+", 
                   RowBox[{
                    RowBox[{"Q", "^", "2"}], "/", 
                    RowBox[{"r", "^", "2"}]}]}], ")"}], "*", 
                 RowBox[{"D", "[", 
                  RowBox[{
                   RowBox[{"Rs", "[", "r", "]"}], ",", 
                   RowBox[{"{", 
                    RowBox[{"r", ",", "2"}], "}"}]}], "]"}]}], "+", 
                RowBox[{
                 RowBox[{"(", "   ", 
                  RowBox[{
                   RowBox[{
                    RowBox[{"2", "/", "r"}], "*", 
                    RowBox[{"(", " ", 
                    RowBox[{"1", "-", 
                    RowBox[{"2", "*", 
                    RowBox[{"M", "/", "r"}]}], "+", 
                    RowBox[{
                    RowBox[{"Q", "^", "2"}], "/", 
                    RowBox[{"r", "^", "2"}]}]}], ")"}]}], " ", "+", " ", 
                   RowBox[{"(", "  ", 
                    RowBox[{
                    RowBox[{"2", "*", 
                    RowBox[{"M", "/", 
                    RowBox[{"r", "^", "2"}]}]}], "-", 
                    RowBox[{"2", "*", 
                    RowBox[{
                    RowBox[{"Q", "^", "2"}], "/", 
                    RowBox[{"r", "^", "3"}]}]}]}], "  ", ")"}]}], " ", ")"}], 
                 "*", 
                 RowBox[{"D", "[", 
                  RowBox[{
                   RowBox[{"Rs", "[", "r", "]"}], ",", 
                   RowBox[{"{", 
                    RowBox[{"r", ",", "1"}], "}"}]}], "]"}]}], "+", 
                RowBox[{
                 RowBox[{"(", "     ", 
                  RowBox[{
                   RowBox[{
                    RowBox[{"1", "/", 
                    RowBox[{"(", 
                    RowBox[{"1", "-", 
                    RowBox[{"2", "*", 
                    RowBox[{"M", "/", "r"}]}], "+", 
                    RowBox[{
                    RowBox[{"Q", "^", "2"}], "/", 
                    RowBox[{"r", "^", "2"}]}]}], ")"}]}], "*", 
                    RowBox[{
                    RowBox[{"(", "  ", 
                    RowBox[{"w", "-", 
                    RowBox[{"qscal", "*", 
                    RowBox[{"Q", "/", "r"}]}]}], " ", ")"}], "^", "2"}]}], 
                   "-", 
                   RowBox[{"l", "*", 
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"l", "+", "1"}], ")"}], "/", 
                    RowBox[{"r", "^", "2"}]}]}], "-", 
                   RowBox[{"mu", "^", "2"}]}], "        ", ")"}], "*", 
                 RowBox[{"Rs", "[", "r", "]"}]}]}], "==", "0"}], ",", 
              RowBox[{
               RowBox[{"Rs", "[", "r0", "]"}], "\[Equal]", 
               RowBox[{
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"rp", "-", "rm"}], ")"}], "^", 
                 RowBox[{"(", 
                  RowBox[{"sigmasol", "-", "rhosol", "-", "1"}], ")"}]}], "*", 
                RowBox[{"Exp", "[", 
                 RowBox[{"qsol", "*", "r0"}], "]"}], "*", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"r0", "-", "rp"}], ")"}], "^", 
                 RowBox[{"(", "rhosol", ")"}]}]}]}], ",", 
              RowBox[{
               RowBox[{
                SuperscriptBox["Rs", "\[Prime]",
                 MultilineFunction->None], "[", "r0", "]"}], "\[Equal]", 
               RowBox[{
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"rp", "-", "rm"}], ")"}], "^", 
                 RowBox[{"(", 
                  RowBox[{"sigmasol", "-", "rhosol", "-", "1"}], ")"}]}], "*", 
                RowBox[{"Exp", "[", 
                 RowBox[{"qsol", "*", "r0"}], "]"}], "*", 
                RowBox[{"(", "rhosol", ")"}], "*", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"r0", "-", "rp"}], ")"}], "^", 
                 RowBox[{"(", 
                  RowBox[{"rhosol", "-", "1"}], ")"}]}]}]}]}], "}"}], ",", 
