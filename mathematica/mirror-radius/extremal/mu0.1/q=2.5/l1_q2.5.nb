Cell[BoxData[{
 RowBox[{
  RowBox[{"winit", "=", 
   RowBox[{"(", 
    RowBox[{"0.3", "-", 
     RowBox[{"0.00005", "*", "I"}]}], ")"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ie", "=", "0"}], ";", 
  RowBox[{"While", "[", 
   RowBox[{
    RowBox[{"ie", "<", "101"}], ",", 
    RowBox[{
     RowBox[{"NITMAX", "=", "250"}], ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"LeavP1", "[", 
       RowBox[{"w_", "?", "NumberQ"}], "]"}], ":=", "\[IndentingNewLine]", 
      RowBox[{"Module", "[", 
       RowBox[{
        RowBox[{"{", "Res", "}"}], ",", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{"r0", "=", 
          RowBox[{"M", "+", 
           RowBox[{"5", "*", 
            RowBox[{"10", "^", 
             RowBox[{"(", 
              RowBox[{"-", "4"}], ")"}]}]}]}]}], ";", 
         RowBox[{"r1", "=", 
          RowBox[{"30", " ", "+", 
           RowBox[{"ie", "/", "5"}]}]}], ";", 
         RowBox[{"wc", "=", "qscal"}], ";", "\[IndentingNewLine]", 
         RowBox[{"qsol", "=", 
          RowBox[{"-", 
           RowBox[{"Sqrt", "[", 
            RowBox[{
             RowBox[{"mu", "*", "mu"}], "-", 
             RowBox[{"w", "*", "w"}]}], "]"}]}]}], " ", ";", 
         "\[IndentingNewLine]", 
         RowBox[{"rhosol", "=", 
          RowBox[{
           RowBox[{"-", "2"}], "*", "I", "*", "M", "*", 
           RowBox[{"(", 
            RowBox[{"w", "-", "wc"}], ")"}]}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"sigmasol", "=", 
          RowBox[{"M", "*", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"qscal", "*", "w"}], "+", 
             RowBox[{"mu", "^", "2"}], "-", 
             RowBox[{"2", "*", 
              RowBox[{"w", "^", "2"}]}]}], ")"}]}]}], ";", 
         "\[IndentingNewLine]", "\[IndentingNewLine]", 
         RowBox[{"sol", "=", 
          RowBox[{"NDSolve", "[", 
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{
               RowBox[{
                RowBox[{"D", "[", 
                 RowBox[{
                  RowBox[{"Rs", "[", "r", "]"}], ",", 
                  RowBox[{"{", 
                   RowBox[{"r", ",", "2"}], "}"}]}], "]"}], "+", 
                RowBox[{"2", "*", 
                 RowBox[{
                  RowBox[{"D", "[", 
                   RowBox[{
                    RowBox[{"Rs", "[", "r", "]"}], ",", 
                    RowBox[{"{", 
                    RowBox[{"r", ",", "1"}], "}"}]}], "]"}], "/", 
                  RowBox[{"(", 
                   RowBox[{"r", "-", "M"}], ")"}]}]}], "+", 
                RowBox[{
                 RowBox[{"(", " ", 
                  RowBox[{
                   RowBox[{
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{
                    RowBox[{"w", "*", 
                    RowBox[{"r", "^", "2"}]}], "-", 
                    RowBox[{"qscal", "*", "M", "*", "r"}]}], ")"}], "^", 
                    "2"}], "/", 
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"r", "-", "M"}], ")"}], "^", "4"}]}], "-", " ", 
                   RowBox[{
                    RowBox[{"1", "/", 
                    RowBox[{
                    RowBox[{"(", 
                    RowBox[{"r", "-", "M"}], ")"}], "^", "2"}]}], "*", 
                    RowBox[{"(", 
                    RowBox[{
                    RowBox[{
                    RowBox[{"mu", "^", "2"}], "*", 
                    RowBox[{"r", "^", "2"}]}], "+", 
                    RowBox[{"l", "*", 
                    RowBox[{"(", 
                    RowBox[{"l", "+", "1"}], ")"}]}]}], ")"}]}]}], "  ", 
                  ")"}], "*", 
                 RowBox[{"Rs", "[", "r", "]"}]}]}], "==", "0"}], ",", 
              RowBox[{
               RowBox[{"Rs", "[", "r0", "]"}], "\[Equal]", 
               RowBox[{
                RowBox[{"M", "^", 
                 RowBox[{"(", 
                  RowBox[{"sigmasol", "-", "rhosol", "-", "1"}], ")"}]}], "*", 
                RowBox[{"Exp", "[", 
                 RowBox[{
                  RowBox[{"qsol", "*", "r0"}], "+", 
                  RowBox[{"I", "*", 
                   RowBox[{"M", "^", "2"}], "*", 
                   RowBox[{
                    RowBox[{"(", 
                    RowBox[{"w", "-", "wc"}], ")"}], "/", 
                    RowBox[{"(", 
                    RowBox[{"r0", "-", "M"}], ")"}]}]}]}], "]"}], "*", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"r0", "-", "M"}], ")"}], "^", 
                 RowBox[{"(", "rhosol", ")"}]}]}]}], ",", 
              RowBox[{
               RowBox[{
                SuperscriptBox["Rs", "\[Prime]",
                 MultilineFunction->None], "[", "r0", "]"}], "\[Equal]", 
               RowBox[{
                RowBox[{"M", "^", 
                 RowBox[{"(", 
                  RowBox[{"sigmasol", "-", "rhosol", "-", "1"}], ")"}]}], "*", 
                RowBox[{"Exp", "[", 
                 RowBox[{
                  RowBox[{"qsol", "*", "r0"}], "+", 
                  RowBox[{"I", "*", 
                   RowBox[{"M", "^", "2"}], "*", 
                   RowBox[{
                    RowBox[{"(", 
                    RowBox[{"w", "-", "wc"}], ")"}], "/", 
                    RowBox[{"(", 
                    RowBox[{"r0", "-", "M"}], ")"}]}]}]}], "]"}], "*", 
                RowBox[{"(", 
                 RowBox[{
                  RowBox[{"rhosol", "*", 
                   RowBox[{"(", 
                    RowBox[{"r0", "-", "M"}], ")"}]}], "-", 
                  RowBox[{"I", "*", 
                   RowBox[{"M", "^", "2"}], "*", 
                   RowBox[{"(", 
                    RowBox[{"w", "-", "wc"}], ")"}]}]}], ")"}], "*", 
                RowBox[{
                 RowBox[{"(", 
                  RowBox[{"r0", "-", "M"}], ")"}], "^", 
                 RowBox[{"(", 
                  RowBox[{"rhosol", "-", "2"}], ")"}]}]}]}]}], "}"}], ",", 
            "Rs", ",", 
            RowBox[{"{", 
             RowBox[{"r", ",", "r0", ",", "r1"}], "}"}], ",", "\n", 
            "          ", 
            RowBox[{"MaxSteps", "\[Rule]", "50000"}]}], "]"}]}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"Res", "=", 
          RowBox[{
           RowBox[{"Rs", "[", "r1", "]"}], "/.", "sol"}]}]}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"wres", "=", 
      RowBox[{
       RowBox[{
        RowBox[{"FindRoot", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"LeavP1", "[", "w", "]"}], "==", "0"}], ",", 
          RowBox[{"{", 
           RowBox[{"w", ",", "winit"}], "}"}]}], "]"}], " ", "[", 
        RowBox[{"[", "1", "]"}], "]"}], "[", 
