         RowBox[{"r1", "=", 
          RowBox[{"30", "+", 
           RowBox[{"ie", "/", "5"}]}]}], ";", " ", 
         RowBox[{"rm", "=", 
          RowBox[{"M", "-", 
           RowBox[{"Sqrt", "[", 
            RowBox[{
             RowBox[{"M", "^", "2"}], "-", 
             RowBox[{"Q", "^", "2"}]}], "]"}]}]}], ";", " ", 
         RowBox[{"rp", "=", 
          RowBox[{"M", "+", 
           RowBox[{"Sqrt", "[", 
            RowBox[{
             RowBox[{"M", "^", "2"}], "-", 
             RowBox[{"Q", "^", "2"}]}], "]"}]}]}], " ", ";", 
         RowBox[{"wc", "=", 
          RowBox[{"qscal", "*", 
           RowBox[{"Q", "/", "rp"}]}]}], ";", "\[IndentingNewLine]", 
         RowBox[{"qsol", "=", 
          RowBox[{"-", 
           RowBox[{"Sqrt", "[", 
            RowBox[{
             RowBox[{"mu", "*", "mu"}], "-", 
             RowBox[{"w", "*", "w"}]}], "]"}]}]}], " ", ";", 
         "\[IndentingNewLine]", 
